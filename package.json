    "scripts": {
        "ng": "./node_modules/.bin/ng",
        "start": "./node_modules/.bin/ng serve --env=serve --proxy-config proxy.js",
        "debug": "./node_modules/.bin/ng serve --env=debug --proxy-config proxy.js",
        "dev:local": "./node_modules/.bin/ng serve --env=dev",
        "test:local": "./node_modules/.bin/ng serve --env=test",
        "build": "./node_modules/.bin/ng build",
        "test": "./node_modules/.bin/ng build --prod --env=test --base-href ./",
        "lint": "./node_modules/.bin/ng lint",
        "e2e": "./node_modules/.bin/ng e2e",
        "dev": "./node_modules/.bin/ng build --env=dev",
        "beta": "./node_modules/.bin/ng build --env=beta",
        "newBeta": "./node_modules/.bin/ng build --env=newBeta",
        "huidu": "./node_modules/.bin/ng build --prod --env=huidu --base-href ./",
        "prod": "./node_modules/.bin/ng build --prod --env=prod --base-href ./",
        "precommit-msg": "echo 'Pre-commit checks...' && ./node_modules/.bin/ng build --prod"
    },
    "pre-commit": [
        "precommit-msg"
    ],
